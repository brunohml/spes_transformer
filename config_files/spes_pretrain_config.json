{
    "data_class": "spes",
    "data_dir": "classification_datasets/SPES/soz_0.3",
    "output_dir": "output/spes/pretrained",
    "records_file": "imputation_records.xls",
    "model": "transformer",
    "task": "imputation",


    "batch_size": 32,
    "max_seq_len": 487,
    "d_model": 128,
    "num_heads": 8,
    "num_layers": 4,
    "dim_feedforward": 256,
    "dropout": 0.1,
    "pos_encoding": "learnable",
    "projection_layer": "conv1d",
    "kernel_width": 15,

    "activation": "gelu",
    "normalization": "preproc_normalized",
    "lr": 1e-5,
    "epochs": 500,
    "optimizer": "Adam",
    "l2_reg": 0.0,
    "num_workers": 4,
    "print_interval": 10,
    "val_ratio": 0,
    "pattern": "TRAIN",
    "val_pattern": "TEST",
    "early_stopping_patience": 10,
    "early_stopping_delta": 0.01,

    "mask_mode": "separate",
    "mask_distribution": "geometric",
    "mean_mask_length": 5,
    "masking_ratio": 0.25
} 